<h1>Edit Item</h1>
<%= form_for @item, :url => {:controller => "items", :action => "update" }  do |f|  %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :price %>
  <%= f.number_field :price %>
  <%= f.submit "Update" %>
<% end %>
